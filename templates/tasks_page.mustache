{{#js}}
    $(document).ready(function(){
        $("#page-theme-cbe-view_tasks #search").on("keyup", function() {
            var value = $(this).val().toLowerCase();

            $(".generaltable tbody tr").filter(function() {
                if (!$(this).hasClass( "emptyrow" )){
                    $(this).toggleClass("hide", $(this).text().toLowerCase().indexOf(value) < 0);
                }
            });
        });
    });
