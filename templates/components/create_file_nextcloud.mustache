<section id="create_file_nextcloud" class="dropdown">
    <button data-action="create_file_nextcloud" class="btn btn-primary dropdown-toggle" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        <i class="fa fa-plus" aria-hidden="true"></i>
        <span>{{# str }} nc_createfile_nc, theme_cbe {{/ str }}</span>
    </button>
    <div class="dropdown-menu" aria-labelledby="dropdownMenu2">
        <button data-action="word" class="dropdown-item" type="button"><i class="fa fa-file-word-o" aria-hidden="true"></i> {{# str }} nc_createfile_word, theme_cbe {{/ str }}</button>
        <button data-action="excel" class="dropdown-item" type="button"><i class="fa fa-file-excel-o" aria-hidden="true"></i> {{# str }} nc_createfile_excel, theme_cbe {{/ str }}</button>
        <button data-action="pp" class="dropdown-item" type="button"><i class="fa fa-file-powerpoint-o" aria-hidden="true"></i> {{# str }} nc_createfile_pp, theme_cbe {{/ str }}</button>
        <button data-action="feedback" class="dropdown-item" type="button"><i class="fa fa-bar-chart" aria-hidden="true"></i> {{# str }} nc_createfile_feedback, theme_cbe {{/ str }}</button>
        <button data-action="form" class="dropdown-item" type="button"><i class="fa fa-check-square-o" aria-hidden="true"></i> {{# str }} nc_createfile_form, theme_cbe {{/ str }}</button>
    </div>
    <span class="notice-filenc small">
        <i class="fa fa-exclamation-triangle" aria-hidden="true"></i> {{# str }} notice_filecreate, theme_cbe {{/ str }}</span>
</section>
{{#js}}

    require(['theme_cbe/createfilenc'], function(createfilenc) {
        createfilenc.initCreatefilenc();
    });

{{/js}}